# AllOSINT API Configuration Example
# Copy this file to .env and fill in your actual API keys

# Cloudflare DNS over HTTPS
CLOUDFLARE_DOH_API=https://cloudflare-dns.com/dns-query

# Certificate Transparency Logs
CERTSPOTTER_API=https://api.certspotter.com/v1
CRTSH_API=https://crt.sh

# WHOIS Services
WHOIS_API=https://www.whois.com/whois

# IP Geolocation
IPAPI_URL=https://ipapi.co
IPGEOLOCATION_API_KEY=your_api_key_here

# Phone Number Validation
LIBPHONENUMBER_CDN=https://cdn.jsdelivr.net/npm/libphonenumber-js@1.10.14/bundle/libphonenumber-js.min.js

# Breach Database
BREACH_API_URL=https://haveibeenpwned.com/api/v3

# Social Media APIs
SOCIAL_SEARCH_API=https://api.social-searcher.com

# Technology Detection
WAPPALYZER_API=https://api.wappalyzer.com

# Google Custom Search (for Dork Generator)
GOOGLE_CSE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_search_engine_id_here

# SSL Certificate APIs
SSL_LABS_API=https://api.ssllabs.com/api/v3

# Port Scanner
PORT_SCANNER_API=your_port_scanner_api_here

# Subdomain Enumeration
SUBDOMAIN_API=https://api.subdomain-finder.com

# DNS Lookup
DNS_LOOKUP_API=https://dns.google/resolve

# Threat Intelligence
KASPERSKY_THREAT_MAP=https://cybermap.kaspersky.com/en/widget/dynamic/dark
ALIENVAULT_OTX_API=https://otx.alienvault.com/api/v1

# Application Settings
APP_VERSION=2.4.0
APP_NAME=AllOSINT
MAX_CREDITS=5000
DEFAULT_CREDITS=2500

